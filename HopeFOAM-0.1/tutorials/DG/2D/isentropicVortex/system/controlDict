FoamFile
{
    version     0.1;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application       dgEulerFoam;

startFrom         startTime;

startTime         0;

stopAt            endTime;

endTime           2;

deltaT            0.008;

writeControl      timeStep;

writeInterval     25;

purgeWrite        0;

writeFormat       ascii;
 
writePrecision    16;

writeCompression  off;

timeFormat        general;

timePrecision     6;

runTimeModifiable true;

adjustTimeStep    no;

maxCo             1;

maxDeltaT         1;

